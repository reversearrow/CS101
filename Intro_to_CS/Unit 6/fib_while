#Fibonacci Numbers using a While Loop


def fibonacci(n):
    zero = 0
    first = 1
    count = 0
    while True:
        second = first + zero
        zero = first
        first = second
        count  += 1
        if n == 0:
            return 0
        if n == 1:
            return 1
        
        if count >= n:
            break
    return zero
